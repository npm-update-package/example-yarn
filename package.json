    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "7.0.4",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "rimraf": "4.1.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
